<?xml version="1.0" encoding="UTF-8" ?>
        <!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.yanghui.mybatics.mapper.DataSetMapper">

<resultMap id="DataSetMap" type="com.yanghui.mybatics.dao.DataSet">
    <id column="id" property="id"/>
    <result column="tenant_id" property="tenantId"/>
    <result column="data_source_id" property="dataSourceId"/>
    <result column="name" property="name"/>
    <result column="api_key" property="apiKey"/>
    <result column="url" property="url"/>
    <result column="recent_time" property="recentTime"/>
    <result column="refresh_time" property="refreshTime"/>
    <result column="req_time" property="reqTime"/>
    <result column="auth_info" property="authInfo"/>
    <result column="basic_auth" property="basicAuth"/>
    <result column="method" property="method"/>
    <result column="body" property="body"/>
    <result column="user_name" property="userName"/>
    <result column="table_name" property="tableName"/>
    <!--<result column="result_field" property="resultField"/>-->
    <result column="description" property="description"/>
    <result column="support_filter" property="supportFilter"/>
    <result column="password" property="password"/>
    <result column="support_time" property="supportTime"/>
    <result column="start_time" property="startTime"/>
    <result column="end_time" property="endTime"/>
    <result column="all_num" property="allNum"/>
    <result column="is_connect" property="isConnect"/>
    <result column="is_delete" property="isDelete"/>
    <result column="created_time" property="createdTime"/>
</resultMap>

<sql id="dataSet">
        `id`,
        `tenant_id`,
        `data_source_id`,
        `name`,
        `api_key`,
        `url`,
        `recent_time`,
        `refresh_time`,
        `basic_auth`,
        `auth_info`,
        `req_time`,
        `body`,
        `user_name`,
        `password`,
        `table_name`,
--         `result_field`,
        `description`,
        `support_filter`,
        `all_num`,
        `is_connect`,
        `start_time`,
        `end_time`,
        `support_time`,
        `method`,
        `is_delete`,
        `created_time`
    </sql>

<select id="listAll" resultMap="DataSetMap">
    select
    <include refid="dataSet"/>
    from data_set
    where `is_delete` = 0
    ORDER BY `created_time` DESC
</select>

</mapper>